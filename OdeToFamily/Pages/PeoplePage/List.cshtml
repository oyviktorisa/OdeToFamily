@page
@model ListModel
@{
}

<h1>People</h1>

<table class="table">
    @foreach(var people in Model.People)
    {
        <tr>
            <td>@people.Name</td>
            <td>@people.NIK</td>
            <td>@people.Gender</td>
        </tr>
    }
</table>
<a asp-page=".\Edit" class="btn bg-primary">Add People</a>

